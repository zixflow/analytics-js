{
  "name": "@zixflow/analytics-js",
  "version": "1.0.3",
  "description": "Zixflow Analytics SDK is built for modern web applications, offering a lightweight and reliable way to track user events, page views, identities, and group behavior. Designed to work seamlessly across frameworks like React, Vue, and plain JavaScript, it comes pre-configured for performance and easy integrationâ€”so you can focus on insights, not setup.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf dist && npm run build",
    "build:bundle": "esbuild src/index.ts --bundle --format=esm --outfile=dist/index.bundle.js --minify",
    "build:all": "npm run build:clean && npm run build:bundle",
    "prepublishOnly": "npm run build:all",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "echo \"No tests yet\"",
    "test:build": "npm run build && node -e \"console.log('Build successful')\"",
    "clean": "rm -rf dist node_modules package-lock.json",
    "prepack": "npm run build:all",
    "publish:package": "./scripts/publish.sh"
  },
  "keywords": [
    "analytics",
    "tracking",
    "events",
    "user-tracking",
    "web-analytics",
    "react",
    "vue",
    "nextjs",
    "typescript",
    "browser",
    "sdk"
  ],
  "type": "module",
  "peerDependencies": {
    "@segment/analytics-next": "^1.0.0"
  },
  "devDependencies": {
    "@segment/analytics-next": "^1.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zixflow/analytics-js.git",
    "directory": "."
  },
  "bugs": {
    "url": "https://github.com/zixflow/analytics-js/issues"
  },
  "homepage": "https://github.com/zixflow/analytics-js#readme",
  "author": "zixflow",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
